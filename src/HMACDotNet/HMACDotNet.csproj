<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>HMACDotNet</Title>
        <Authors>Samuel Lucas</Authors>
        <Description>A .NET implementation of HMAC, with support for SHA256, SHA384, and SHA512.</Description>
        <Copyright>Copyright (c) 2022 Samuel Lucas </Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/samuel-lucas6/HMAC.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/samuel-lucas6/HMAC.NET</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>hmac sha256 sha384 sha512 sha2 hmac-sha256 hmac-sha384 hmac-sha512 mac message-authentication-code</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <FileVersion>1.0.0.0</FileVersion>
    </PropertyGroup>

</Project>
